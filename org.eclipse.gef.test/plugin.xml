<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.gef.test"
   name="GEF JUnit Tests"
   version="3.2.0"
   provider-name="Eclipse.org"
   class="org.eclipse.gef.test.GEFTestPlugin">

   <runtime>
      <library name="geftests.jar"/>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime" />
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.draw2d"/>
      <import plugin="org.eclipse.gef"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.test.performance" optional="true"/>
      <import plugin="org.eclipse.gef.examples.logic" optional="true"/>
      <import plugin="org.eclipse.ui.views" optional="true"/>
   </requires>

</plugin>
