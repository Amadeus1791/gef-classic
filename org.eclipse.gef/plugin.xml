<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="org.eclipse.gef"
   name="Graphical Editor Framework"
   version="2.0.1"
   provider-name="Eclipse.org">

   <runtime>
      <library name="runtime/gef.jar">
         <export name="org.eclipse.gef.*"/>
         <export name="org.eclipse.gef.dnd.*"/>
         <export name="org.eclipse.gef.commands.*"/>
         <export name="org.eclipse.gef.requests.*"/>
         <export name="org.eclipse.gef.editparts.*"/>
         <export name="org.eclipse.gef.editpolicies.*"/>
         <export name="org.eclipse.gef.handles.*"/>
         <export name="org.eclipse.gef.palette.*"/>
         <export name="org.eclipse.gef.tools.*"/>
         <export name="org.eclipse.gef.ui.actions.*"/>
         <export name="org.eclipse.gef.ui.palette.*"/>
         <export name="org.eclipse.gef.ui.parts.*"/>
         <export name="org.eclipse.gef.ui.properties.*"/>
         <export name="org.eclipse.gef.ui.console.*"/>
         <export name="org.eclipse.gef.ui.stackview.*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.draw2d" export="true" version="2.0.1"/>
      <import plugin="org.eclipse.core.runtime" export="true"/>
      <import plugin="org.eclipse.core.resources" export="true"/>
      <import plugin="org.eclipse.ui" export="true"/>
   </requires>


</plugin>
