<project name="BuildAll" default="build" basedir="../">

<target name="draw2d">
	<ant antfile="org.eclipse.draw2d.doc.isv/build.xml" target="build"/>
</target>

<target name="gef">
	<ant antfile="org.eclipse.gef.doc.isv/build.xml" target="build"/>
</target>

<target name="javadoc">
	<ant antfile="org.eclipse.gef.doc.isv/javadocAll.xml" target="start"/>
</target>

<target name="build">
	<antcall target="draw2d"/>
	<antcall target="gef"/>
	<echo message="Build complete." />
</target>

</project>

