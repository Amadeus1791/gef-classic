<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="org.eclipse.gef.doc.isv"
   name="GEF Documentation"
   version="2.0.1"
   provider-name="Eclipse.org">

   <runtime>
   </runtime>
   <requires>
      <import plugin="org.eclipse.gef" version="2.0.0"/>
   </requires>


   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="topics_Guide.xml">
      </toc>
      <toc
            file="topics_Reference.xml">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="%GEF_Developer_views_category_name"
            id="org.eclipse.gef">
      </category>
      <view
            name="%GEF_Debug_view_name"
            icon="icons/debugGEF.gif"
            category="org.eclipse.gef"
            class="org.eclipse.gef.ui.console.DebugGEF"
            id="GEF Debug console">
      </view>
      <view
            name="%GEF_stackviewer_view_name"
            icon="icons/debugGEF.gif"
            category="org.eclipse.gef"
            class="org.eclipse.gef.ui.stackview.CommandStackInspector"
            id="GEF CommandStack View">
      </view>
   </extension>

</plugin>
